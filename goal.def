/* The fields in the cpp macro call "DEF_GOAL()" are:

   1. The internal name of the goal function.

   2. The number of input operands.

   3. The printed name of the instruction.

   4. C code that implements the function.

   */

DEF_GOAL (P1,             1, "p1",          { r = (v0-1) & v0; })
DEF_GOAL (P2,             1, "p2",          { r = (v0+1) & v0; })
DEF_GOAL (P3,             1, "p3",          { r = (-v0)  & v0; })
DEF_GOAL (P4,             1, "p4",          { r = (v0-1) ^ v0; })
DEF_GOAL (P5,             1, "p5",          { r = (v0-1) | v0; })
DEF_GOAL (P6,             1, "p6",          { r = (v0+1) | v0; })
DEF_GOAL (P7,             1, "p7",          { r = (v0+1) & ~v0; })
DEF_GOAL (P8,             1, "p8",          { r = (v0-1) & ~v0; })
DEF_GOAL (P9,             1, "p9",          { r = ((signed_word) v0 < 0)?-v0:v0; })
DEF_GOAL (P10,            2, "p10",         { r =  (v0^v1) <= (v0&v1); })
DEF_GOAL (P11,            2, "p11",         { r =  (v0&~v1) > v1 ; })
DEF_GOAL (P12,            2, "p12",         { r =  (v0&~v1) <= v1 ; })
DEF_GOAL (P13,            1, "p13",         { r = (signed_word) v0 > 0 ? 1 : ((signed_word) v0 < 0 ? -1 : 0); })
DEF_GOAL (P14,            2, "p14",         { r =  (v0&v1) + ((v0^v1)>>1) ; })
DEF_GOAL (P15,            2, "p15",         { r =  (v0|v1) - ((v0^v1)>>1) ; })
DEF_GOAL (P16,            2, "p16",         { r =  (v0 > v1)?v0:v1 ; })
DEF_GOAL (P17,            1, "p17",         { r = (((v0-1) | v0) + 1) & v0; })
DEF_GOAL (P18,            1, "p18",         { r = !((v0-1) & v0) && v0; })
DEF_GOAL (P19,            3, "p19",         { unsigned_word r__o3 = ((v0 >> v2) ^ v0) & v1; r = ((r__o3 << 1) ^ r__o3) ^ v0; })
DEF_GOAL (P20,            3, "p20",         { unsigned_word r__o2 = v0 & -v0; \
                                              unsigned_word r__o3 = r__o2 + v0; \
                                              unsigned_word r__o3a = ((v0 ^ r__o2) >> 2); \
                                                            r = (r__o2?r__o3a / r__o2:0) | r__o3; })
DEF_GOAL (P21,            4, "p21",         { r = (v0 == v1)? v2 : ((v0 == v2)?v3:v1); })
DEF_GOAL (P22,            1, "p22",         { r =  __builtin_popcountl(v0)&1 ; })
DEF_GOAL (P23,            1, "p23",         { r =  __builtin_popcountl(v0) ; })
DEF_GOAL (P24,            1, "p24",         { r =  (v0 <= 2)? v0 : (1 << (sizeof(v0)*8 - __builtin_clz(v0 - 1))) ; })
DEF_GOAL (P25,            2, "p25",         ({ word t1; word t2; umul_ppmm (t1, t2, v0, v1); r = t2; }))

