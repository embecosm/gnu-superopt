/* Maximum of 32 for compatability */
#define N_BITS             ((sizeof(unsigned_word)*8) > 32 ? 32 : (sizeof(unsigned_word)*8))
/* Negative high one */
#define HIGHONE_N         ((unsigned_word)1<<(N_BITS-1))
/* Positive high one */
#define HIGHONE_P         ((signed_word)1<<(N_BITS-2))
DEF_GOAL (EQ80000000,            2, "eq80000000",         { r = HIGHONE_N & -(v0 == v1); })
DEF_GOAL (NE80000000,            2, "ne80000000",         { r = HIGHONE_N & -(v0 != v1); })
DEF_GOAL (LES80000000,           2, "les80000000",        { r = HIGHONE_N & -((signed_word) v0 <= (signed_word) v1); })
DEF_GOAL (GES80000000,    2, "ges80000000",   { r = HIGHONE_N & -((signed_word) v0 >= (signed_word) v1); })
DEF_GOAL (LTS80000000,    2, "lts80000000",   { r = HIGHONE_N & -((signed_word) v0 < (signed_word) v1); })
DEF_GOAL (GTS80000000,    2, "gts80000000",   { r = HIGHONE_N & -((signed_word) v0 > (signed_word) v1); })
DEF_GOAL (LEU80000000,    2, "leu80000000",   { r = HIGHONE_N & -((unsigned_word) v0 <= (unsigned_word) v1); })
DEF_GOAL (GEU80000000,    2, "geu80000000",   { r = HIGHONE_N & -((unsigned_word) v0 >= (unsigned_word) v1); })
DEF_GOAL (LTU80000000,    2, "ltu80000000",   { r = HIGHONE_N & -((unsigned_word) v0 < (unsigned_word) v1); })
DEF_GOAL (GTU80000000,    2, "gtu80000000",   { r = HIGHONE_N & -((unsigned_word) v0 > (unsigned_word) v1); })

DEF_GOAL (EQ080000000,    1, "eq080000000",   { r = HIGHONE_N & -(v0 == 0); })
DEF_GOAL (NE080000000,    1, "ne080000000",   { r = HIGHONE_N & -(v0 != 0); })
DEF_GOAL (LES080000000,   1, "les080000000",  { r = HIGHONE_N & -((signed_word) v0 <= 0); })
DEF_GOAL (GES080000000,   1, "ges080000000",  { r = HIGHONE_N & -((signed_word) v0 >= 0); })
DEF_GOAL (LTS080000000,   1, "lts080000000",  { r = HIGHONE_N & -((signed_word) v0 < 0); })
