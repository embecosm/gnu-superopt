DEF_GOAL (GRAY2,          1, "gray2",       { v0 = ((~(v0 ^ (v0 << 1)) & (-1 << (N_BITS-1))) | ((v0 << 1) & ~(-1 << (N_BITS-1))) | (v0 >> (N_BITS-1))); r = ((~(v0 ^ (v0 << 1)) & (-1 << (N_BITS-1))) | ((v0 << 1) & ~(-1 << (N_BITS-1))) | (v0 >> 31)); })
